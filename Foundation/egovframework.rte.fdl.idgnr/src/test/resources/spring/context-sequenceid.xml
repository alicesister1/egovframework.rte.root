<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	<bean name="Ids-TestSequenceNonExistingSequenceName" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrService" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query"  value="SELECT nonexisting-sequence.NEXTVAL FROM DUAL"/>
	</bean>
	<bean name="Ids-TestSequenceSimpleRequestIdsSize1" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrService" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query"  value="SELECT idstest.NEXTVAL FROM DUAL"/>
	</bean>
	<bean name="Ids-TestSequenceMaxByteIds" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrService" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="query"  value="SELECT idstest.NEXTVAL FROM DUAL"/>
	</bean>
	<bean name="Ids-TestSequenceMaxBigDecimalIds" class="egovframework.rte.fdl.idgnr.impl.EgovSequenceIdGnrService" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="useBigDecimals" value="true"/>
		<property name="query"  value="SELECT idstest.NEXTVAL FROM DUAL"/>
	</bean>	
</beans>	
